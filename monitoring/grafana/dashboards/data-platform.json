{
  "dashboard": {
    "title": "FastTrader Data Platform",
    "tags": ["data-platform", "etl", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 36,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "title": "System Overview",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
        "id": 1
      },
      {
        "title": "Total Tables",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 0, "y": 1},
        "id": 2,
        "targets": [
          {
            "expr": "sum(data_platform_parquet_tables_total) + data_platform_clickhouse_tables_total",
            "legendFormat": "Total Tables"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "textMode": "value_and_name"
        }
      },
      {
        "title": "Total Records",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 4, "y": 1},
        "id": 3,
        "targets": [
          {
            "expr": "sum(data_platform_parquet_rows_total) + data_platform_clickhouse_rows_total",
            "legendFormat": "Total Records"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      },
      {
        "title": "ClickHouse Status",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 8, "y": 1},
        "id": 4,
        "targets": [
          {
            "expr": "data_platform_clickhouse_connected",
            "legendFormat": "Connected"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "background",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "1", "text": "Connected"},
              {"type": "value", "value": "0", "text": "Disconnected"}
            ],
            "thresholds": {
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "title": "Redis Status",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 12, "y": 1},
        "id": 5,
        "targets": [
          {
            "expr": "data_platform_redis_connected",
            "legendFormat": "Connected"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "background"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "1", "text": "Connected"},
              {"type": "value", "value": "0", "text": "Disconnected"}
            ],
            "thresholds": {
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "title": "Data Quality Score",
        "type": "gauge",
        "gridPos": {"h": 4, "w": 8, "x": 16, "y": 1},
        "id": 6,
        "targets": [
          {
            "expr": "data_platform_data_quality_score{layer=\"overall\"}",
            "legendFormat": "Quality"
          }
        ],
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 75, "color": "yellow"},
                {"value": 90, "color": "green"}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "title": "Storage Metrics",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 5},
        "id": 7
      },
      {
        "title": "Tables by Layer",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
        "id": 8,
        "targets": [
          {
            "expr": "data_platform_parquet_tables_total{layer=\"bronze\"}",
            "legendFormat": "Bronze"
          },
          {
            "expr": "data_platform_parquet_tables_total{layer=\"silver\"}",
            "legendFormat": "Silver"
          },
          {
            "expr": "data_platform_parquet_tables_total{layer=\"gold\"}",
            "legendFormat": "Gold"
          },
          {
            "expr": "data_platform_clickhouse_tables_total",
            "legendFormat": "ClickHouse"
          }
        ],
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["lastNotNull"]},
          "tooltip": {"mode": "multi"}
        }
      },
      {
        "title": "Rows by Layer",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
        "id": 9,
        "targets": [
          {
            "expr": "data_platform_parquet_rows_total{layer=\"bronze\"}",
            "legendFormat": "Bronze"
          },
          {
            "expr": "data_platform_parquet_rows_total{layer=\"silver\"}",
            "legendFormat": "Silver"
          },
          {
            "expr": "data_platform_parquet_rows_total{layer=\"gold\"}",
            "legendFormat": "Gold"
          },
          {
            "expr": "data_platform_clickhouse_rows_total",
            "legendFormat": "ClickHouse"
          }
        ]
      },
      {
        "title": "Storage Size by Layer",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14},
        "id": 10,
        "targets": [
          {
            "expr": "data_platform_parquet_size_bytes{layer=\"bronze\"} / 1024 / 1024",
            "legendFormat": "Bronze (MB)"
          },
          {
            "expr": "data_platform_parquet_size_bytes{layer=\"silver\"} / 1024 / 1024",
            "legendFormat": "Silver (MB)"
          },
          {
            "expr": "data_platform_parquet_size_bytes{layer=\"gold\"} / 1024 / 1024",
            "legendFormat": "Gold (MB)"
          },
          {
            "expr": "data_platform_clickhouse_size_bytes / 1024 / 1024",
            "legendFormat": "ClickHouse (MB)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "decmbytes"
          }
        }
      },
      {
        "title": "Redis Cache Performance",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14},
        "id": 11,
        "targets": [
          {
            "expr": "data_platform_redis_hit_rate",
            "legendFormat": "Hit Rate %",
            "yAxisIndex": 0
          },
          {
            "expr": "data_platform_redis_keys_total",
            "legendFormat": "Total Keys",
            "yAxisIndex": 1
          }
        ],
        "fieldConfig": {
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Hit Rate %"},
              "properties": [{"id": "unit", "value": "percent"}]
            }
          ]
        }
      },
      {
        "title": "ETL Pipeline",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 22},
        "id": 12
      },
      {
        "title": "ETL Jobs Status",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 23},
        "id": 13,
        "targets": [
          {
            "expr": "data_platform_etl_jobs_total",
            "legendFormat": "Total"
          },
          {
            "expr": "data_platform_etl_jobs_scheduled",
            "legendFormat": "Scheduled"
          },
          {
            "expr": "data_platform_etl_jobs_running",
            "legendFormat": "Running"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "colorMode": "value"
        }
      },
      {
        "title": "ETL Job Execution Rate",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 18, "x": 6, "y": 23},
        "id": 14,
        "targets": [
          {
            "expr": "rate(data_platform_etl_job_executions_total{status=\"success\"}[5m])",
            "legendFormat": "Success Rate"
          },
          {
            "expr": "rate(data_platform_etl_job_executions_total{status=\"failure\"}[5m])",
            "legendFormat": "Failure Rate"
          }
        ]
      },
      {
        "title": "ETL Job Duration",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 27},
        "id": 15,
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(data_platform_etl_job_duration_seconds_bucket[5m])) by (job_name, le))",
            "legendFormat": "{{job_name}} p95"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(data_platform_etl_job_duration_seconds_bucket[5m])) by (job_name, le))",
            "legendFormat": "{{job_name}} p50"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "title": "Records Processed",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 27},
        "id": 16,
        "targets": [
          {
            "expr": "rate(data_platform_etl_records_processed_total[5m])",
            "legendFormat": "{{job_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "rps"
          }
        }
      },
      {
        "title": "API Performance",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 35},
        "id": 17
      },
      {
        "title": "Request Rate",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 36},
        "id": 18,
        "targets": [
          {
            "expr": "sum(rate(data_platform_api_requests_total[5m])) by (endpoint)",
            "legendFormat": "{{endpoint}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        }
      },
      {
        "title": "API Latency (p95)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 36},
        "id": 19,
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(data_platform_api_request_duration_seconds_bucket[5m])) by (endpoint, le))",
            "legendFormat": "{{endpoint}} p95"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(data_platform_api_request_duration_seconds_bucket[5m])) by (endpoint, le))",
            "legendFormat": "{{endpoint}} p50"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "title": "Error Rate",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 44},
        "id": 20,
        "targets": [
          {
            "expr": "sum(rate(data_platform_api_requests_total{status=~\"5..\"}[5m])) by (endpoint) / sum(rate(data_platform_api_requests_total[5m])) by (endpoint) * 100",
            "legendFormat": "{{endpoint}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            }
          }
        }
      }
    ]
  }
}
